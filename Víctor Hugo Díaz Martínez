void draw(){
  background(204);
  for (int i=1; i<=5; i=i+1){  //esta parte saca columnas
    for (int j=1; j<=i; j=j+1){ //esta parte nos da filas, j serÃ¡ dependiente de i
      rect(j*100, i*100, 50, 50);
    }
  }
}
  
